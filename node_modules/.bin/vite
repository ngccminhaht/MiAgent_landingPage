#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/AHT/MI-Agent/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_7364807e34650a05b45f27cb625ede94/node_modules/vite/bin/node_modules:/mnt/d/AHT/MI-Agent/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_7364807e34650a05b45f27cb625ede94/node_modules/vite/node_modules:/mnt/d/AHT/MI-Agent/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_7364807e34650a05b45f27cb625ede94/node_modules:/mnt/d/AHT/MI-Agent/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/AHT/MI-Agent/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_7364807e34650a05b45f27cb625ede94/node_modules/vite/bin/node_modules:/mnt/d/AHT/MI-Agent/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_7364807e34650a05b45f27cb625ede94/node_modules/vite/node_modules:/mnt/d/AHT/MI-Agent/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_7364807e34650a05b45f27cb625ede94/node_modules:/mnt/d/AHT/MI-Agent/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
