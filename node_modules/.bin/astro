#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/AHT/MI-Agent/node_modules/.pnpm/astro@5.15.4_@types+node@24_d54dd435da7a1a773961db605b1ac93b/node_modules/astro/node_modules:/mnt/d/AHT/MI-Agent/node_modules/.pnpm/astro@5.15.4_@types+node@24_d54dd435da7a1a773961db605b1ac93b/node_modules:/mnt/d/AHT/MI-Agent/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/AHT/MI-Agent/node_modules/.pnpm/astro@5.15.4_@types+node@24_d54dd435da7a1a773961db605b1ac93b/node_modules/astro/node_modules:/mnt/d/AHT/MI-Agent/node_modules/.pnpm/astro@5.15.4_@types+node@24_d54dd435da7a1a773961db605b1ac93b/node_modules:/mnt/d/AHT/MI-Agent/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
